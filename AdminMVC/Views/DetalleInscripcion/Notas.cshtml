
@{
    ViewBag.Title = "Notas";
    Layout = "~/Views/Shared/_LayoutProfe.cshtml";
    var id = ViewBag.IdProfesor;
}
<input type="hidden" id="MiId" value="@id"/>
<center><h3 class="text-light">Mis Alumnos</h3></center>
<div class="container">
    <div class="row">
        <div class="col-12 text-white">
            <input type="text" id="buscar" name="Buscar" class="form-control espacio-btn" placeholder="Haz una busqueda"/><br>
            <table id="alumnos" class="table table-dark table-hover table-responsive">
                <thead class="thead-dark">
                    <tr>
                        <th>Nombres</th>
                        <th>Apellidos</th>
                        <th>Codigo</th>
                        <th>Ciclo</th>
                        <th>Opciones</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
<script src="~/Scripts/JSprofesor/crudProfesor.js"></script>